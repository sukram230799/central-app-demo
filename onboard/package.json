    "svelte": "^3.55.1",
    "swiper": "^8.4.6"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "cpy-cli": "^4.2.0",
    "cross-env": "^7.0.3",
